{
  "name": "misc-test-utils",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "esr src/index.ts",
    "tsc": "tsc -p tsconfig.json",
    "clean": "rm -rf *.zip test-files/*.csv zip-files/*.zip node_modules && npm ci && npm run start",
    "test": "jest",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.2.127",
    "@swc/jest": "^0.2.15",
    "@types/archiver": "^5.1.0",
    "@types/form-data": "^2.5.0",
    "@types/inquirer": "^7.3.2",
    "@types/node": "^15.14.9",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chalk": "^4.1.1",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.23.4",
    "gen-env-types": "^1.3.0",
    "inquirer": "^8.1.1",
    "jest": "^27.4.7",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "axios": "^0.21.2",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "esbuild": "^0.13.12",
    "esbuild-runner": "^2.2.1",
    "form-data": "^4.0.0",
    "formdata-node": "^3.5.4",
    "globby": "^11.0.4",
    "node-stream-zip": "^1.13.6",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.{ts,tsx,js,jsx}",
      "<rootDir>/*.test.{ts,tsx,js,jsx}"
    ],
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "sourceMaps": true,
          "jsc": {
            "parser": {
              "syntax": "typescript",
              "tsx": true
            },
            "transform": {
              "react": {
                "runtime": "automatic"
              }
            }
          }
        }
      ]
    },
    "testEnvironment": "node"
  }
}
