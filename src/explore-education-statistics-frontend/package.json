    "referrer-policy": "^1.1.0",
    "use-immer": "^0.4.0",
    "webpack": "^4.42.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/jest-dom": "^5.8.0",
    "@types/classnames": "^2.2.10",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/memoizee": "^0.4.3",
    "@types/node": "^13.11.0",
    "@types/react": "16.9.32",
    "@types/react-beautiful-dnd": "^12.1.2",
    "@types/react-dom": "16.9.6",
    "@types/react-leaflet": "^2.5.1",
    "@types/recharts": "^1.8.9",
    "@types/testing-library__react": "^10.0.1",
    "@types/testing-library__react-hooks": "^3.2.0",
    "@types/testing-library__jest-dom": "^5.7.0",
    "@types/whatwg-fetch": "0.0.33",
    "@types/yup": "^0.26.34",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "explore-education-statistics-common": "0.1.0",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "^25.2.7",
    "jest-junit": "^10.0.0",
    "jest-resolve": "^25.2.6",
    "jest-serializer-html": "^7.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "ncp": "^2.0.0",
    "normalize.css": "^8.0.1",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "react-test-renderer": "16.13.1",
    "sass": "^1.26.3",
    "stylelint": "^13.3.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "typescript": "^3.8.3",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "start": "node server.js",
    "start:local": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "build": "next build ./src",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint src/**/*.{ts,tsx}",
    "lint:style": "stylelint src/**/*.{scss,css}",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "tsc": "tsc --noEmit -p src/tsconfig.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/test/**/*.(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/../explore-education-statistics-common/test/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/../explore-education-statistics-common/test/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@frontend/(.*)$": "<rootDir>/src/$1",
      "^@common/(.*)$": "<rootDir>/../explore-education-statistics-common/src/$1",
      "^@common-test/(.*)$": "<rootDir>/../explore-education-statistics-common/test/$1",
      "^react$": "<rootDir>/node_modules/react",
      "^formik$": "<rootDir>/node_modules/formik"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ],
    "resetMocks": true,
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "jest-junit": {
    "ancestorSeparator": " â€º ",
    "suiteName": "Frontend Jest tests",
    "outputName": "junit-frontend.xml"
  }
}
