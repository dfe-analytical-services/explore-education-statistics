{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "storageProvider": {
        "type": "AzureStorage",
        "connectionStringName": "DurableManagementStorage"
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Error",
      "Function": "Information"
    }
  }
}
