{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Warning",
      "Microsoft": "Warning",
      "Function": "Information",
      "GovUk.Education.ExploreEducationStatistics": "Information",
      "Azure": "Error"
    }
  },
  "extensions": {
    "queues": {
      "maxDequeueCount": 1
    }
  }
}
