{
  "ConnectionStrings": {
    "PublicDataDb": "Host=db;Username=app_public_data_api;Password=password;Database=public_data"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "MicroElements.Swashbuckle.FluentValidation": "Warning"
    }
  },
  "App": {
    "Url": "http://localhost:5050",
    "EnableSwagger": true
  },
  "AzureSearchClient": {
    "Endpoint": "<search service endpoint e.g. https://[search service name].search.windows.net>",
    "Credential": {
      "Key": "<query API key>"
    },
    "IndexName": "<index name>"
  },
  "ContentApi": {
    "Url": "http://localhost:5010"
  },
  "MiniProfiler": {
    "Enabled": true
  },
  "DataFiles": {
    "BasePath": "data/public-api-data"
  },
  "Analytics": {
    "Enabled": false,
    "BasePath": "c:\\temp\\analytics"
  }
}
