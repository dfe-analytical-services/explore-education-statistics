{
  "version": "2.0",
  "functionTimeout": "04:00:00",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Warning",
      "Microsoft": "Warning",
      "Function": "Information",
      "GovUk.Education.ExploreEducationStatistics": "Information",
      "Azure": "Error"
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 6,
      "maxDequeueCount": 3,
      "newBatchThreshold": 1,
      "maxPollingInterval": "00:00:02"
    }
  }
}
